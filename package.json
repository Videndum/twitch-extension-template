{
  "name": "@videndum/twitch-extension-template",
  "version": "0.1.0",
  "description": "A template for twitch extensions using expo with game overlay & memoryjs built-in. Create without hassle",
  "game": "Notepad",
  "repository": {
    "type": "git",
    "directory": "https://github.com/videndum"
  },
  "homepage": "https://github.com/videndum",
  "bugs": {
    "email": "bugs@videndum.studio",
    "url": "https://github.com/videndum"
  },
  "author": {
    "email": "jonathan@videndum.studio",
    "name": "Jonathan Stevens",
    "url": "https://jonathanstevens.org"
  },
  "contributors": [],
  "license": "",
  "private": false,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "npm install --global windows-build-tools && cd node_modules/memoryjs && npm run build32 && npm run build64",
    "build:storybook": "build-storybook",
    "dev:format": "npm run dev:format-scripts",
    "dev:format-check": "prettier --config .prettier.json --check .",
    "dev:format-scripts": "prettier --config .prettier.json --write .",
    "eject": "expo eject",
    "start": "concurrently npm:start:web npm:start:desktop",
    "start:android": "expo start --android",
    "start:default": "expo start",
    "start:desktop": "npm run tsc:electron && yarn expo-electron start",
    "start:ios": "expo start --ios",
    "start:storybook": "start-storybook -p 6006",
    "start:web": "expo start --web",
    "tsc:electron": "tsc -p tsconfig.electron.json",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.12.6",
    "concurrently": "^5.3.0",
    "electron": "^6.0.12",
    "electron-overlay-window": "^1.0.4",
    "expo": "~40.0.0",
    "expo-asset": "~8.2.1",
    "expo-constants": "~9.3.0",
    "expo-font": "~8.4.0",
    "expo-linking": "~2.0.0",
    "expo-splash-screen": "~0.8.0",
    "expo-status-bar": "~1.0.3",
    "expo-web-browser": "~8.6.0",
    "express": "^4.17.1",
    "memoryjs": "^3.4.0",
    "node-gyp-build": "^4.2.3",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.0",
    "react-native-web": "~0.13.12",
    "react-twitch-ext-onauthorized": "^2.0.3",
    "twitch-ebs-tools": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@expo/electron-adapter": "^0.0.39",
    "@expo/webpack-config": "^0.12.52",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/react-native": "^5.3.23",
    "@types/express": "^4.17.11",
    "@types/react": "~16.9.35",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "~0.63.2",
    "babel-loader": "^8.2.2",
    "destiny": "^0.7.1",
    "electron-builder": "^22.9.1",
    "jest-expo": "~40.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "typescript": "~4.0.0"
  },
  "gypfile": true,
  "jest": {
    "preset": "jest-expo"
  },
  "readme": "readme.md"
}